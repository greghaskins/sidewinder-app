{
  "name": "sidewinder-app",
  "version": "1.0.0",
  "description": "sidewinder-app: An Ionic project",
  "private": true,
  "dependencies": {
    "gulp": "^3.5.6",
    "gulp-sass": "^1.3.3",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.3.13",
    "bower": "^1.3.3",
    "cordova": "^5.0.0",
    "gulp-util": "^2.2.14",
    "ionic": "^1.4.8",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.35",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.2.0",
    "phantomjs": "^1.9.17",
    "shelljs": "^0.3.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "com.ionic.keyboard"
  ],
  "cordovaPlatforms": [
    "ios"
  ],
  "scripts": {
    "prestart": "npm install",
    "start": "ionic serve --lab",
    "pretest": "npm install",
    "test": "npm run karma:headless",
    "tdd": "npm run karma:watch",
    "ionic": "./node_modules/ionic/bin/ionic",
    "karma": "./node_modules/karma/bin/karma start test/karma.conf.js",
    "karma:watch": "npm run karma -- --auto-watch --no-single-run",
    "karma:headless": "npm run karma -- --single-run --no-auto-watch"
